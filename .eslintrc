{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "plugin:react/jsx-runtime",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "array-callback-return": "off",
    "react/prop-types": "off",
    "no-useless-return": "off",
    "no-unused-vars": "warn",
    "react/jsx-no-useless-fragment": "off"
  }
}
